<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Othmane N. - Portfolio</title>
    <link rel="icon" type="image/png" href="img/favicon.png">
    <!--Stylesheet-->
    <link rel="stylesheet" href="css/core.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
    <!--Javascript-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW" crossorigin="anonymous"></script>
    <script src="js/core.js"></script>
    <script src="libraries/vue/vue.js"></script>
</head>

<body id="app" class="lm-body">
    <main>
        <section id="main-sec">
            <div id="navbar">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-sm ps-3">
                            <img v-bind:src="logo.path" v-bind:alt="logo.alt" v-bind:width="logo.size">
                        </div>
                        <div class="col-5 d-flex justify-content-center align-items-center">
                            <div>
                                <ul id="nav">
                                    <a v-bind:href="navLinks.l1">
                                        <li v-text="nav.n1" v-bind:class="navClass.c1"></li>
                                    </a>
                                    <a v-bind:href="navLinks.l2">
                                        <li v-text="nav.n2" v-bind:class="navClass.c2"></li>
                                    </a>
                                    <a v-bind:href="navLinks.l3">
                                        <li v-text="nav.n3" v-bind:class="navClass.c3"></li>
                                    </a>
                                    <a v-bind:href="navLinks.l4">
                                        <li v-text="nav.n4" v-bind:class="navClass.c4"></li>
                                    </a>
                                </ul>
                            </div>
                        </div>
                        <div class="col-sm d-flex justify-content-end align-items-center">
                            <div class="row">
                                <ul>
                                    <li>
                                        <a v-bind:href="searchIcon.href" v-html="searchIcon.path" v-bind:class="searchIcon.c"></a>
                                    </li>
                                    <li>
                                        <a v-bind:href="contactBtn.href" v-text="contactBtn.text" v-bind:class="contactBtn.c"></a>
                                    </li>
                                    <li>
                                        <span class="mode-btn">
                                          <a v-html="modeBtn.dark.path" v-bind:href="modeBtn.dark.href" v-bind:alt="modeBtn.dark.alt" v-bind:class="modeBtn.dark.class" v-on:click="dark();"></a>
                                          <a v-html="modeBtn.light.path" v-bind:href="modeBtn.light.href" v-bind:alt="modeBtn.light.alt" v-bind:class="modeBtn.light.class"></a>
                                        </span>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="navbar-mobile" class="p-4">
                <div class="d-flex">
                    <div class="col">
                        <img v-bind:src="logo.path" v-bind:alt="logo.alt" v-bind:width="logo.size">
                    </div>
                    <div class="col pt-2">
                        <div class="float-end mt-2 m-ico-c" dir="rtl">
                            <a href="#" v-on:click="menushow();">
                                <div class="m-ico m-ico-1"></div>
                                <div class="m-ico m-ico-2"></div>
                                <div class="m-ico m-ico-3"></div>
                            </a>
                        </div>
                    </div>
                </div>
                <div id="nc-mob" class="navbar-mobile-menu hide">
                    <div class="d-flex p-4">
                        <div class="col">
                            <div>
                                <img v-bind:src="logo.path" v-bind:alt="logo.alt" v-bind:width="logo.size">
                            </div>
                        </div>
                        <div class="col">
                            <div class="float-end m-ico-c" dir="rtl">
                                <a href="#" v-on:click="menuhide();">
                                    <div class="m-ico-close m-ico-close-1"></div>
                                    <div class="m-ico-close m-ico-close-2"></div>
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="text-center">
                        <a v-bind:href="nav.n1.href">
                            <h1>{{nav.n1.txt}}</h1>
                        </a>
                        <a v-bind:href="nav.n2.href">
                            <h1>{{nav.n2.txt}}</h1>
                        </a>
                        <a v-bind:href="nav.n3.href">
                            <h1>{{nav.n3.txt}}</h1>
                        </a>
                        <a v-bind:href="nav.n4.href">
                            <h1>{{nav.n4.txt}}</h1>
                        </a>
                        <div>
                            <ul class="list-inline">
                                <li class="list-inline-item">
                                    <a v-bind:href="sm.instagram.href" v-html="sm.instagram.icon"></a>
                                </li>
                                <li class="list-inline-item">
                                    <a v-bind:href="sm.github.href" v-html="sm.github.icon"></a>
                                </li>
                                <li class="list-inline-item">
                                    <a v-bind:href="sm.linkedin.href" v-html="sm.linkedin.icon"></a>
                                </li>
                                <li class="list-inline-item">
                                    <a v-bind:href="sm.behance.href" v-html="sm.behance.icon"></a>
                                </li>
                            </ul>
                        </div>
                        <div class="source-code">
                            <h1>Sourced on <a v-bind:href="sm.github.repo" target="_blank">Github</a></h1>
                            <ul class="list-inline">
                                <li class="list-inline-item" v-html="sm.vue.icon"></li>
                                <li class="list-inline-item" v-html="sm.bootstrap.icon"></li>
                                <li class="list-inline-item" v-html="sm.wp.icon"></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div id="content" class="d-flex justify-content-center">
                <div class="text-center">
                    <div v-html="headline.text" v-bind:class='headline.c'></div>
                    <div class="img-con">
                        <img v-bind:src="img.path" v-bind:width="img.size" v-bind:alt="img.alt">
                        <div class="cursor-img">
                            <img src="img/hello.gif" alt="">
                        </div>
                    </div>
                    <br>
                    <a v-bind:href="btn.href" v-bind:class="btn.c" v-html="btn.html"></a>
                </div>
            </div>
        </section>
        <section id="abm-sec">
            <div v-bind:class="headline.c">
                <p>{{headline.mutedtxt}}</p>
                <h1>{{headline.txt}}</h1>
            </div>
            <div v-bind:class="overview.c" v-html="overview.txt"></div>
            <div class="container-fluid">
                <div class="row">
                    <div class="col" v-html="icon.left.path"></div>
                    <div class="col-8">
                        <ul class="list-inline text-center">
                            <li class="list-inline-item"><img v-bind:src="lang.html.path" v-bind:alt="lang.html.alt" v-bind:width="lang.html.size"></li>
                            <li class="list-inline-item"><img v-bind:src="lang.css.path" v-bind:alt="lang.css.alt" v-bind:width="lang.css.size"></li>
                            <li class="list-inline-item"><img v-bind:src="lang.bootstrap.path" v-bind:alt="lang.bootstrap.alt" v-bind:width="lang.bootstrap.size"></li>
                            <li class="list-inline-item"><img v-bind:src="lang.js.path" v-bind:alt="lang.js.alt" v-bind:width="lang.js.size"></li>
                            <li class="list-inline-item"><img v-bind:src="lang.jquery.path" v-bind:alt="lang.jquery.alt" v-bind:width="lang.jquery.size"></li>
                            <li class="list-inline-item"><img v-bind:src="lang.scss.path" v-bind:alt="lang.scss.alt" v-bind:width="lang.scss.size"></li>
                            <li class="list-inline-item"><img v-bind:src="lang.bulma.path" v-bind:alt="lang.bulma.alt" v-bind:width="lang.bulma.size"></li>
                            <li class="list-inline-item"><img v-bind:src="lang.materialize.path" v-bind:alt="lang.materialize.alt" v-bind:width="lang.materialize.size"></li>
                        </ul>
                    </div>
                    <div class="col text-right" v-html="icon.right.path"></div>
                </div>
            </div>
        </section>
    </main>
    <!--Vue.js-->
    <script src="js/core.vue.js"></script>
    <script>
        const cursor = document.querySelector('.cursor-img');
        document.addEventListener('mousemove', e => {
            cursor.setAttribute("style", "top: " + (e.pageY) + "px; left:" + (e.pageX) + "px;");
        });
    </script>
</body>

</html>